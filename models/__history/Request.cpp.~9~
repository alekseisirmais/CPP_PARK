#include "Request.h"

int MakeRequest(User user, int id) {
	switch(id) {
		case GET_USER_INFO:

			return 0;
			break;
		case GET_USB_DEVICES_INFO:
			return 0;
			break;

		case ADD_NEW_USER:
			return 0;
			break;
		case ADD_NEW_USB_DEVICE:
			return 0;
			break;

		case DELETE_USER:
			return 0;
			break;
		case DELETE_USB_DEVICE:
			return 0;
			break;
	}
	return -1;

}
