#include "User.h"

User::User() {
	id = -1;
	identificationKey = 0;
	login = "";
}

User::~User() {}

User::User(const User& other) {
	id = other.id;
	login = other.login;
}

User& User::operator=(const User& other) {
	id = other.id;
	login = other.login;
}

long long User::GetId() {
	return id;
}

UnicodeString User::GetLogin() {
	return login;
}

void User::SetId(const long long value) {
	id = value;
}

long long User::SetIdKey(long long value) {
	identificationKey = value;
}

void User::SetLogin(UnicodeString loginStr) {
	login = loginStr;
}
